<style>
   .trust-list .trust-part .trust-title {
     font-size: 16px;
     font-weight: 700;
   }
  .trust-textbody{
    font-size:14px;
  }
  .trust-list {
     display: flex;
     flex-wrap: wrap;
     flex-direction: row;
     justify-content: space-between;
   }
   .trust-part-items{
     display:flex;
     align-items:center;
   }
   .trust-part-items .trust-icon svg{
     fill:#0bbc08;
     width:24px;
   }
   .trust-part-items .trust-icon{
     padding:10px 5px 10px 0;

   }
   .trust-part-items-pop{
     position:absolute;
     padding:15px;
     width:210px;
     left:50%;
     top:-5px;
     transform:translate(-50%,-100%);
     background-color:rgb(9,108,165);
     display:none;
     z-index:1000;
     color:#fff;
   }
   .trust-part-items-pop a{
     color:#fff;
     text-decoration:underline;
   }
     .trust-list .trust-part{
     width:32%;
   }
   .trust-list .trust-part:first-of-type{
     width:42%;
   }
  .trust-list .trust-part:last-of-type{
     width:26%;
   }
   .trust-part-items-pop::after{
     content:"";
     width:0;
     height:0;
     border:15px solid transparent;
     border-top:15px solid rgb(9,108,165);
     position:absolute;
     bottom:-29px;
     left:50%;
     transform:translate(-50%);

   }
   .trust-part-items{
     cursor:pointer;
   }
   .trust-part-items-pop div:first-of-type{
       margin-bottom:10px;
   }
    .trust-part-items-pop div:last-of-type{
       margin-bottom:5px;
   }
   .trust-list .trust-part{
     position:relative;
   }
   .trust-list .trust-part:not(:last-child):after {
       content: "";
       position: absolute;
       top: 15%;
       left: 95%;
       display: inline-block;
       height: 45px;
       width: 1px;
       background-color: #e4e9ed;
   }
  .product-details__block{
    margin-top:10px;
  }
   @media screen and (max-width: 1079px){
     .trust-part-items-pop{
       padding:10px;
     }
     .trust-part-items-pop div:first-of-type{
       font-size:14px;
     }
     .trust-part-items-pop div:last-of-type,.trust-part-items-pop a{
       font-size:12px;
     }

     .product-details__block .trust-list{
         max-width:100%;
     }
     .trust-list .trust-part{
       width:32%;

     }
     .trust-list .trust-part:first-of-type .trust-part-items-pop{
       left:0;
       transform:translate(0,-100%);
     }
     .trust-list .trust-part:first-of-type .trust-part-items-pop::after{
       left:15%;
     }
     .trust-list .trust-part:last-of-type .trust-part-items-pop{
       left:auto;
       right:0;
       transform:translate(0,-100%);
     }
     .trust-list .trust-part:last-of-type .trust-part-items-pop::after{
       left:auto;
       right:20%;
     }
     .trust-list .trust-part:first-of-type{
       width:43%;
       flex-shrink:0;
     }
     .trust-list .trust-part:last-of-type{
       width:25%;
       flex-shrink:0;
     }
     .trust-list .trust-part .trust-icon{
       padding:10px 3px 10px 0;
     }
     .trust-list .trust-part .trust-part-items .trust-icon svg{
         width:20px;
     }
     .trust-list .trust-part .trust-title,.trust-list .trust-part .trust-textbody{
       font-size:10px;
     }
     .trust-list .trust-part:not(:last-child):after {
         left: 97.5%;
     }
   }
</style>
