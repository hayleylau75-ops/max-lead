                        "spec_right_align": false,
                        "spec_show_empty_metafields": false,
                        "spec_empty_field_text": "-",
                        "tab_1_title": "Quick Guide & Support",
                        "tab_1_text": "<p>{{ product.metafields.custom.quick_guide_support.value }}</p>",
                        "tab_1_page": "",
                        "tab_2_title": "Features",
                        "tab_2_text": "{{ product.metafields.custom.features | metafield_tag }}",
                        "tab_2_page": "",
                        "tab_3_title": "",
                        "tab_3_text": "",
                        "tab_3_page": ""
                    }
                },
                "payment-methods": {
