        "details": {
            "type": "product-details-gomaxlead",
            "blocks": {
                "highlight-text": {
                    "type": "highlight-text",
                    "settings": {
                        "title": "{{ product.metafields.custom.highlight_text1.value.title.value }}",
                        "text": "<p></p><p>{{ product.metafields.custom.highlight_text1.value.text | metafield_tag }}</p>"
                    }
                },
                "tabs": {
                    "type": "tabs",
                    "settings": {
                        "style": "collapsible",
