<script>
  window.addEventListener("load",function(){
       $(".trust-part").click(function(){
         if($(this).find(".trust-part-items-pop").css("display") != "block"){
           $(".trust-part").find(".trust-part-items-pop").fadeOut()
           $(this).find(".trust-part-items-pop").fadeIn()
         }
       })
       $("body").click(function(e){
         if(!$(e.target).is(".trust-part *")){
           $(".trust-part").find(".trust-part-items-pop").fadeOut()
         }
       })
  })
</script>
